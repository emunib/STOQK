{
  "name": "stoqk",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run css",
    "css-compile": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 SASS/ -o public/css",
    "css-prefix": "postcss --replace public/css/style.css --use autoprefixer --map",
    "css": "npm-run-all css-compile css-prefix",
    "server": "node ./bin/www",
    "start": "npm-run-all --parallel watch server",
    "watch": "nodemon -e js,html,hbs,sass -x \"npm run css\""
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "^4.1.2",
    "http-errors": "~1.6.3",
    "jquery": "^3.6.0",
    "morgan": "~1.9.1",
    "node-sass-middleware": "0.11.0",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.13",
    "postcss-cli": "^8.3.1"
  }
}
